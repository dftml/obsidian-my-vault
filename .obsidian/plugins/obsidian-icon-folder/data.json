{
  "settings": {
    "migrated": 3,
    "iconPacksPath": ".obsidian/icons",
    "fontSize": 16,
    "emojiStyle": "none",
    "iconColor": null,
    "recentlyUsedIcons": [],
    "recentlyUsedIconsSize": 5,
    "rules": [
      {
        "rule": ".*",
        "icon": "FasFolder",
        "for": "everything",
        "order": 0
      },
      {
        "rule": "([a-zA-Z0-9\\s_\\\\.\\-\\(\\):])+(.md)$",
        "icon": "IbDocumentCode",
        "for": "everything",
        "order": 1
      },
      {
        "rule": "assets",
        "icon": "FasImages",
        "for": "everything",
        "order": 2
      }
    ],
    "extraMargin": {
      "top": 0,
      "right": 4,
      "bottom": 0,
      "left": 0
    },
    "iconInTabsEnabled": false,
    "iconInTitleEnabled": false,
    "iconInFrontmatterEnabled": false,
    "iconsBackgroundCheckEnabled": false,
    "iconsInNotesEnabled": true
  }
}